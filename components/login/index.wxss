.login_box{
  position: fixed;
  top: 0;
  width: 100vw;
  height: 100vh;
  z-index: 10;
  background:rgba(24, 24, 24, 0.404);
  display: flex;
  justify-content: center;
  align-items: center;
}

.login_from{
  width: 75vw;
  background: #fff;
  border-radius:20rpx;
  text-align: center;
  padding: 50rpx 0;
  position: relative;
}
.index-btn{
  padding: 0;
  margin:0 ;
  margin-top: 50rpx;
  width: 492rpx;
  height: 112rpx;
  font-size: 32rpx;
  font-weight: 400;
  border-radius: 56rpx;
  line-height: 112rpx;
  box-sizing: content-box;
  background: #35c91d;
  box-shadow: 0px 5rpx 51rpx 0rpx rgba(49, 160, 67, 0.28);
  color: #fff;
}
.close_login{
  position: absolute;
  top: 10rpx;
  right: 30rpx;
  font-size: 56rpx;
  padding: 0 10rpx;
}