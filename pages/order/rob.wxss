.ad_box{
  width: calc(100%);
  height: 270rpx;
  border-radius: 10rpx;
  overflow: hidden;
  margin: 20rpx auto 1rpx;
}
.wedding_img{
 width: 100%;
 display: block;
 height: 100%;
}

.rob_box{
  margin-top: 20rpx;
  width: calc(100% - 60rpx);
}

.rob_item_box{
  position: relative;
  height: 300rpx;
  margin: 20rpx auto 0;
  border-radius: 10rpx;
  clear: both;
}
.rob_item_bg{
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  z-index: -1;
}

.rob-price-txt{
    font-size: 30rpx;
    font-family: PingFang SC;
    font-weight: 500;
    font-style: italic;
    color: #FF8B3C;
    display: block;
}

.icon_next{
    display: block;
    width: 30rpx;
    height: 30rpx;
}
.rob_addr{
    font-size: 32rpx;
    font-family: PingFang SC;
    font-weight: 400;
    color: #333333;
    overflow: hidden;
    display: -webkit-box;
    -webkit-line-clamp: 3;
    -webkit-box-orient: vertical;
    width: 100%;
}
.rob_info_box{
  width: 60vw;
  padding-top: 38rpx;
  margin-left: 40rpx;
}

.rob_num{
  position: absolute;
  right: 0;
  top: 31rpx;
  width: 139rpx;
  height: 68rpx;
  background: linear-gradient(-90deg, #FF973E, #FF342C);
  border: 0px solid #2ABDBB;
  border-radius: 34rpx 0 0 34rpx;
  line-height: 68rpx;
  text-align: center;
  color: #ffffff;
  font-size: 38rpx;
  font-style: italic;
}

.empty-class{
  height: calc(100vh - 340rpx);
}
.rob-bookTime{
    font-size: 24rpx;
    font-family: PingFang SC;
    font-weight: 400;
    color: #333333;
    margin-top: 6rpx;
}

.rob_price_box{
    width: 264rpx;
    height: 56rpx;
    border-radius: 28rpx;
    position: absolute;
    bottom: 32rpx;
    right: 23rpx;
    background: #FFF7F0;
    border: 1px solid #F9B512;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    align-items: center;
}
.price-box-rf{
    width: 134rpx;
    height: 106%;
    background: linear-gradient(-90deg, #FF973E, #FF342C);
    border-radius: 28rpx;
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
    position: absolute;
    top: -1px;
    right: -1px;
    bottom: 0;
}

.price-box-lf{
    width: 136rpx;
    text-align: center;
}

.box-rf-txt{
    font-size: 30rpx;
    font-family: PingFang SC;
    font-weight: 500;
    color: #FFFFFF;
    margin-left: 6rpx;
    margin-right: 8rpx;
}


.modal{
  position: fixed;
  width: 100%;
  height: 100%;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  background: rgba(0,0,0,.3);
  box-sizing: border-box;
}

.modal_body{
  position: fixed;
  width: 690rpx;
  background: #fff;
  border-radius: 16rpx;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%);
}

.modal_title{
  width: 100%;
  height: 120rpx;
  display: flex;
  align-items: center;
  justify-content: center;  
  font-size: 36rpx;
  color: #333;
}

.modal_text{
  display: flex;
  height: 145rpx;
  box-sizing: border-box;
  padding: 20rpx 0;
  flex-direction: column;
  justify-content: space-between;
  align-items: center;
  font-size: 30rpx;
  color: #333;
  border-top: 1rpx solid #f2f2f2;
  border-bottom: 1rpx solid #f2f2f2;
}

.modal_package{
  width: 100%;
  padding: 24rpx;
  box-sizing: border-box;
}

.modal_type_title{
  width: 100%;
  color: #333;
  font-size: 24rpx;
  margin-bottom: 10rpx
}

.modal_type_package{
  width: 100%;
  background: #F2F2F2;
  display: flex;
  flex-wrap: wrap;
  padding: 24rpx 16rpx 12rpx;
  box-sizing: border-box;
}

.modal_type_list{
  padding: 0 20rpx;
  background: #fff;
  border-radius: 8rpx;
  border: 1rpx solid #aaa;
  box-sizing: border-box;
  margin-bottom: 12rpx;
  margin-right: 12rpx;
  font-size: 30rpx;
  display: flex;
}

.modal_type_name{
  color: #31A043;
}

.modal_type_num{
  color: #333;
  margin: 0 10rpx;
}

.modal_type_unit{
  color: #333;
  font-size: 24rpx;
}

.modal_confirm{
  width: 100%;
  height: 88rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 30rpx;
  border-top: 1rpx solid #f2f2f2;
  color: #333;
}