page{
    background: #f3f4f5;
  }

.vipColor {
    /* color: #ff4553; */
    color: #f4362a !important;
}

.strong {
    font-size: 36rpx;
    color: #333;
}

.section {
    width: 704rpx;
    margin: 24rpx auto 10rpx;
    background-color: #fff;
    border-radius: 14rpx;
    padding: 18rpx 24rpx;
    box-sizing: border-box;
}

.section .name {
    font-size: 32rpx;
    color: #333;
}

.section .rf {
    display: flex;
    align-items: center;
}

.section .strong {
    font-size: 36rpx;
    color: #333;
}

.section .desc {
    color: rgb(123, 123, 123);
    font-size: 30rpx;
}

.section .li {
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 12rpx 0;
    position: relative;
}

